<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/navbarbackground"
        android:layout_width="426dp"
        android:layout_height="101dp"
        android:layout_marginBottom="4dp"
        android:adjustViewBounds="true"
        android:background="#E7F9EF"
        android:contentDescription="@string/todo1"
        android:contextClickable="false"
        android:cropToPadding="false"
        android:src="@drawable/navbar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />


    <ImageView
        android:id="@+id/navbarbackground1"
        android:layout_width="321dp"
        android:layout_height="393dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="228dp"
        android:adjustViewBounds="true"
        android:background="#E7F9EF"
        android:contentDescription="@string/todo1"
        android:contextClickable="false"
        android:cropToPadding="false"
        android:src="@drawable/navbar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/my_button2"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="261dp"
        android:layout_height="53dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="300dp"
        android:background="#FFFFFF"
        android:drawableLeft="@drawable/camerarollicon"
        android:drawablePadding="10dp"
        android:text="Camera Roll"
        android:textColor="#212121"
        android:textSize="20sp"
        app:iconTint="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/page5"
        android:layout_width="234dp"
        android:layout_height="46dp"
        android:layout_marginStart="96dp"
        android:layout_marginBottom="456dp"
        android:text="Choose Outfit From"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/navbarbackground"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/backbutton"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:background="#E7F9EF"
        android:contentDescription="@string/a_tiny_android_icon"
        android:src="@drawable/backbutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/camerabutton2"
        android:layout_width="121dp"
        android:layout_height="80dp"
        android:layout_marginStart="144dp"
        android:layout_marginBottom="16dp"
        android:background="#E7F9EF"
        android:contentDescription="A tiny Android icon"
        android:src="@drawable/navigation_foreground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/cameraicon"
        android:layout_width="85dp"
        android:layout_height="79dp"
        android:layout_marginStart="72dp"
        android:layout_marginBottom="444dp"
        android:background="#657E66"
        android:contentDescription="A tiny Android icon"
        android:src="@drawable/cameraicon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:layout_width="121dp"
        android:layout_height="80dp"
        android:layout_marginTop="148dp"
        android:background="#E7F9EF"
        android:contentDescription="A tiny Android icon"
        android:src="@drawable/closet_foreground"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/navbarbackground1" />

</androidx.constraintlayout.widget.ConstraintLayout>